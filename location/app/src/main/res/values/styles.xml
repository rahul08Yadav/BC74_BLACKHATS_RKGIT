<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="FrontTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#9b9b9b</item>
        <item name="colorPrimaryDark">#7b7b7b</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">#7b7b7b</item>
    </style>

    <style name="startTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/brown</item>
        <item name="colorPrimaryDark">@color/brown</item>
        <item name="android:navigationBarColor">@color/brown</item>
    </style>


</resources>
