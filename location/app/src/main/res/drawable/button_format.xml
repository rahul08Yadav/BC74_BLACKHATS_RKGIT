<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_enabled="true" >
        <shape>
            <solid android:color="#ffcfbf"/>
            <stroke
                android:width="3dp"
                android:color="#ffcfbf" />
            <corners
                android:radius="50dp" />

        </shape>
    </item>
    <item android:state_enabled="false" >
        <shape>
            <solid android:color="#fde9e2"/>
            <stroke
                android:width="3dp"
                android:color="#ffcfbf" />
            <corners
                android:radius="50dp" />

        </shape>
    </item>

</selector>